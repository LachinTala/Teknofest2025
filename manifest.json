{
  "manifest_version": 3,
  "name": "VSign",
  "version": "1.0",
  "description": "Sign language translator for videos",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "icons/icon.png"
  }
}
